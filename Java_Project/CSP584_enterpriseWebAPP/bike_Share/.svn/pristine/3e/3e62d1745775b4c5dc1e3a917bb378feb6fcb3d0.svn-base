$(document).ready(function () {
    $("#confirm-orders").click(function () {
        
        var goodsid=$('#goodsid').val();
        var flag=$('#flag').val();
        var isPay = $('#pay-select').val();
      
        $.ajax({
            url: "ShowAddOrder",
            type: "POST",
            dataType:"json",
            data: {
                flag:flag,
                isPay: isPay,
                goodsid:goodsid,
            },
            
            success: function (result) {
            	
            	if(result.code==100){
                swal("租车成功", "", "success");
               // location.href = "info/list"
            	}else{
            		 swal(result.msg, "", "error");
            	}
            },
            error: function () {
                swal("购买失败，无法连接到服务器！");
            }
        });
    });
});